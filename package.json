{
  "name": "ripplr",
  "version": "0.1.0",
  "scripts": {
    "test": "NODE_PATH=grails-app/assets/javascripts mocha --compilers es6:babel-register \"src/test/javascript/**/*.es6\"",
    "lint": "jshint grails-app/assets/javascripts"
  },
  "author": "Joel Cochran",
  "repository": "https://github.com/joelforjava/ripplr-grails3",
  "devDependencies": {
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "expect": "23.6.0",
    "jshint": "2.11.0",
    "mocha": "5.2.0",
    "redux-devtools": "3.4.1",
    "webpack": "4.22.0"
  },
  "jshintConfig": {
    "strict": "implied",
    "curly": true,
    "esversion": 5,
    "globals": {
      "window": true,
      "document": true,
      "console": true,
      "location": true
    }
  }
